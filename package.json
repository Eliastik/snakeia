{
  "name": "snakeia",
  "version": "2.2.0",
  "description": "A Snake version with an artificial intelligence. This version has many game modes. You can play against the AI, or let it play alone. A Battle Royale mode is also available, this mode sees between 2 and 100 AI fight! Many settings are available to vary the games. A Levels mode (for the player and the AI) is also available, it's a series of level with a particular goal to achieve (get a minimum score, get a score in time, â€¦). Downloadable levels are available.",
  "main": "dist/SnakeIA-node.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "build": "webpack --config prod.webpack.config.js",
    "build-dev": "webpack --config dev.webpack.config.js",
    "watch": "webpack --config dev.webpack.config.js --watch",
    "start": "webpack-dev-server --config dev.webpack.config.js --open",
    "train-ai": "node train-ai.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eliastik/snakeia.git"
  },
  "author": "Eliastik",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Eliastik/snakeia/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "restoreMocks": true,
    "testEnvironment": "jsdom",
    "clearMocks": true,
    "resetMocks": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "homepage": "https://github.com/Eliastik/snakeia#readme",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.27.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "babel-jest": "^30.0.4",
    "babel-loader": "^10.0.0",
    "cli-progress": "^3.12.0",
    "eslint": "^9.30.1",
    "eslint-webpack-plugin": "^5.0.2",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "i18next": "^25.3.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsgametools": "git+https://github.com/Eliastik/JSGameTools.git#cf5bbf3",
